pragma solidity ^0.8.17;

contract Avax {
   
    // uint public num = 0;
    uint b=5;

    function test(uint num) public pure{
        assert(num!=0);
    }

    function sub(uint a) public view returns (uint){
        require(a>0);
        return a-b;

    }

    function division (uint _numerator, uint _denomenator) public pure returns (uint){
        if(_numerator<_denomenator){
           
            revert("please provide a numerator greater than the denominator");
            
        }
        return _numerator/_denomenator;
       

    }
    

}
